  </dependencySets>
  <fileSets>
    <fileSet>
      <directory>target/classes</directory>
      <outputDirectory></outputDirectory>
      <excludes>
        <exclude>descriptor.xml</exclude>
      </excludes>
    </fileSet>
  </fileSets>
